
@{
    ViewBag.Title = "SarinaPrice";    
}



@using (Html.BeginForm("SarinaPrice", "Hotel", FormMethod.Post, new { enctype = "multipart/form-data", id = "frm" }))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal col-md-9">
        <h3>قیمت گذاری هتل سارینا مشهد</h3>

        <div class="form-group">
            <label class="control-label col-md-2">قیمت</label>
            <div class="col-md-9">
                <input type="text" value="@ViewBag.Price" name="price" class="form-control" />
            
            </div>

        </div>       
        <div class="form-group">
            <div class="col-md-offset-1 col-md-9">
                <input type="submit" value=@ParvazPardaz.Resource.General.Generals.Update class="btn btn-custom green" />
            </div>
        </div>
    </div>
}



