    <script src="~/Plugins/momentDate/moment.min.js"></script>
    <script src="~/Plugins/momentDate/moment-jalaali.js"></script>


    @*@Scripts.Render("~/Plugins/fa-IR/js")*@

    @*@Html.RenderScripts()*@
