    <script src="~/Content/assets/plugins/bootstrap-colorpalette/js/bootstrap-colorpalette.js"></script>
    <script src="~/Content/assets/plugins/Select2_V4/js/select2.js"></script>
    <script src="~/Content/assets/js/main.js"></script>
    @*@Scripts.Render("~/Plugins/js")*@
    <script src="~/Plugins/bootstrap-touchspin-master/bootstrap-touchspin-master/src/jquery.bootstrap-touchspin.js"></script>
    <script src="~/Plugins/jQuery.filer-master/js/jquery.filer.js"></script>
    <script src="~/Content/assets/plugins/bootstrap-fileupload/bootstrap-fileupload.min.js"></script>
