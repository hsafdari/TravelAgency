@model List<ParvazPardaz.Model.Entity.Core.Menu>
@helper ShowTree(IEnumerable<ParvazPardaz.Model.Entity.Core.Menu>
        menuitems)
{
    foreach (var item in menuitems)
    {
        if (item != null)
        {
            // -Parent , -Child --> Root
            if (item.MenuChilds.Count == 0 && item.MenuParentId == null)
                {
                    <li class="@(item.MenuUrl == Request.Url.LocalPath ? "active" : "")">
                        <a href="@item.MenuUrl" target="@item.Target">
                            <span class="title">@item.MenuTitle</span>
                        </a>
                    </li>
                }
                // -Parent , +Child --> Root
                else if (item.MenuChilds.Count > 0 && item.MenuParentId == null)
                {
                    <li class="@(item.MenuChilds.Any(x=>x.MenuUrl.Contains(Request.Url.LocalPath)) ? "active open" : "")">
                        <a href="javascript:void(0)">
                            <i class="clip-screen"></i>
                            <span class="title">@item.MenuTitle</span><i class="icon-arrow"></i>
                            <span class="selected"></span>
                        </a>
                        <ul class="sub-menu">
                            @ShowTree(item.MenuChilds)
                        </ul>
                    </li>
                }
                // +Parent , -Child  --> Child
                else if (item.MenuChilds.Count == 0 && item.MenuParentId != null)
                {
                    <li class="@(item.MenuUrl == Request.Url.LocalPath ? "active" : "")">
                        <a href="@item.MenuUrl" target="@item.Target">
                            <span class="title">@item.MenuTitle</span>
                        </a>
                    </li>
                }
                // +Parent , +Child  --> Child
                else if (item.MenuChilds.Count > 0 && item.MenuParentId != null)
                {
                    <li class="@(item.MenuChilds.Any(x=>x.MenuUrl.Contains(Request.Url.LocalPath)) ? "active open" : "")">
                        <a href="javascript:void(0)">
                            <i class="clip-screen"></i>
                            <span class="title">@item.MenuTitle</span><i class="icon-arrow"></i>
                            <span class="selected"></span>
                        </a>
                        <ul class="sub-menu">
                            @ShowTree(item.MenuChilds)
                        </ul>
                    </li>
                }
           
        }
    }
}

