                                <h2 id="CommentSection">نقد و بررسی کاربران</h2>
                            </div>
                            <!-- Section Header /- -->

                            <ul class="commentlist">
                                @if (Model.CommentList != null && Model.CommentList.Any())
                                {
                                    foreach (var cmt in Model.CommentList.ToList())
                                    {

                                        var cmtIndex = Model.CommentList.IndexOf(cmt);

                                        <li>
                                            <div class="comment">
                                                <span class="comment-info d-text-c">
                                                    <span>@string.Format("{0} در {1:yyyy/MM/dd}", cmt.Name, cmt.CreatorDateTime) </span>
                                                    @cmt.Subject
                                                    @Html.Rate("CmtRate_" + @cmt.Id.ToString()).IsReadonly(true).Rating(Model.CommentList[cmtIndex].Rate).StarWidth("15px").NumberStar(5).Precision(2)
                                                    <span class="rating" id="rating_@cmt.Id">
                                                        @Ajax.ActionLink(".", "LikeComment", "Blog", new { commentId = cmt.Id }, new AjaxOptions() { OnBegin = "IsBeSubmit(this)", OnSuccess = "LikeDislikeSuccess" }, new { @class = "like sprite sprite-fa-thumbs-up-grey", Style = "color: transparent;", id = "like_" + cmt.Id.ToString() })
                                                        <i class="likes">@cmt.Like</i>
                                                        @Ajax.ActionLink(".", "DislikeComment", "Blog", new { commentId = cmt.Id }, new AjaxOptions() { OnBegin = "IsBeSubmit(this)", OnSuccess = "LikeDislikeSuccess" }, new { @class = "dislike sprite sprite-fa-thumbs-down-grey", Style = "color: transparent;", id = "dislike_" + cmt.Id.ToString() })
                                                        @*<a style="cursor:pointer" class="dislike sprite sprite-fa-thumbs-down-grey"></a>*@
                                                        <i class="dislikes">@cmt.DisLike</i>
                                                    </span>
                                                </span>

                                                <p>
                                                    @Html.Raw(cmt.CommentText)
                                                </p>
                                                <div>
                                                    <ul class="list-group" style="font-size:12px !important; width:50%; margin:auto">

                                                        @for (int i = 0; i < cmt.CommentReviews.Count(); i++)
                                                        {
                                                            @Html.HiddenFor(x => x.CommentList[cmtIndex].CommentReviews[i].ReviewId)
                                                            <li class="list-group-item">
                                                                @Model.CommentList[cmtIndex].CommentReviews[i].Title
                                                                <span style="float:left; direction:ltr">
                                                                    @Html.RateFor("CmtReviewRate_" + @cmtIndex.ToString() + "_" + @i.ToString(), model => model.CommentList[cmtIndex].CommentReviews[i].Rate).StarWidth("20px").IsCollectionItem(true).IsReadonly(true).Rating(Model.CommentList[cmtIndex].CommentReviews[i].Rate.Value).NumberStar(5).Precision(2)
                                                                </span>
                                                            </li>
                                                        }

                                                    </ul>
                                                </div>
                                                @*<a class="comment-reply-link d-text-c" href="#"><i class="fa fa-plus"></i> بیشتر</a>*@
                                            </div>
                                        </li>
                                    }
                                }

