    <script type="text/javascript">
        function SuccessLogin(result) {
            //debugger
            if (result == "Success") {
                //واکشی فرم درج نقد
                try {
                    $.ajax({
                        async: false,
                        type: "POST",
                        url: "/Blog/InitialCommentForm",
                        data: {
                            commentType: '@ParvazPardaz.Model.Enum.CommentType.Hotel',
                            ownId: @Model.Id,
                            parentId:0
                        },
                        success: function (fetchedForm) {
                            $("#SectionComment").html(fetchedForm);
                            document.getElementById("frm-login").reset();
                            $('#login-modal').modal('toggle');
                        },
                        error: function (textStatus, errorThrown) {
                            toastr.options = { "positionClass": "toast-top-center" }
                            toastr.error('کاربر گرامی! مرورگر شما مورد نظر دچار مشکل شده است ، لطفا از مرورگر دیگری استفاده کنید', 'درج نقد', { timeOut: 5000 })
                        }
                    });
                } catch (e) {
                    toastr.options = { "positionClass": "toast-top-center" }
                    toastr.error('کاربر گرامی! مرورگر شما مورد نظر دچار مشکل شده است ، لطفا از مرورگر دیگری استفاده کنید', 'درج نقد', { timeOut: 5000 })
                }
            }
            else if (result == "Invalid") {
                toastr.options = { "positionClass": "toast-top-center" }
                toastr.error('کاربر گرامی! اطلاعات ورودی را مورد بررسی قرار دهید', 'ورود به سیستم', { timeOut: 5000 })
                document.getElementById("frm-login").reset();
            }
            else if (result == "Banned") {
                document.getElementById("frm-login").reset();
                toastr.options = { "positionClass": "toast-top-center" }
                toastr.error('کاربر با اطلاعات وارد شده مسدود گردیده است', 'ورود به سیستم', { timeOut: 5000 })
            }
            else if (result == "Failure") {
                document.getElementById("frm-login").reset();
