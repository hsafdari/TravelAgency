                        <div class="col-md-4 col-sm-6 hidden-xs widget-sidebar">
                            <div class="widget-inner author-rating">
                                <h4 style="text-align:center">امتیاز کاربران</h4>
                                <b>@string.Format("از بین {0} نفر", Model.CommentTotalScore.PersonCount)</b>
                                <i class="rating">@Model.CommentTotalScore.TotalRate.ToString("F1")</i>
                                <p>این هتل را پیشنهاد می دهید؟ با امتیاز دادن به این هتل دیگران را از میزان رضایت خود از هتل و خدمات آن مطلع سازید.</p>
                                <div class="about-skill-progres">
                                    @if (Model.CommentTotalScore.TotalCommentReviews != null && Model.CommentTotalScore.TotalCommentReviews.Any())
                                    {
                                        Random rnd = new Random();

                                        foreach (var item in Model.CommentTotalScore.TotalCommentReviews)
                                        {
                                            System.Drawing.Color randomColor = System.Drawing.Color.FromArgb(rnd.Next(256), rnd.Next(256), rnd.Next(256));

                                            <div class="skill-progress-box">
                                                <h3 class="block-title">@item.Title <span id="skill_type2_count-1">@item.Rate</span></h3>
                                                <div class="progress">
                                                    <div id="skill_bar2_count-1" class="progress-bar progress-bar-danger photoshop" role="progressbar" data-color="rgb(@randomColor.A, @randomColor.R, @randomColor.G,@randomColor.B);" style="width: @(item.Rate * 20)%; background-color: rgb(@randomColor.A, @randomColor.R, @randomColor.G,@randomColor.B);">
                                                        <span class="sr-only">@string.Format("{0}% Complete", item.Rate * 20)</span>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                    }
                                </div>
                            </div>
                        </div>
                    }

