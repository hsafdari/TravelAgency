<body data-offset="200" data-spy="scroll" data-target=".primary-navigation">
    <h2 class="hidden">@ViewBag.Title | کی سفر</h2>
    <div id="site-loader" class="load-complete">
        <div class="load-position">
            <div class="logo"><img src="/ClientTheme/images/Logo/keysafarlogo.png" alt="کی سفر" id="logo" /></div>
            <p class="h6 text-center">لطفا صبر کنید...</p>
            <div class="loading">
                <div class="loading-line"></div>
                <div class="loading-break loading-dot-1"></div>
                <div class="loading-break loading-dot-2"></div>
                <div class="loading-break loading-dot-3"></div>
            </div>
        </div>
    </div>

    <div id="scroll-top" style="display:none"><i class="fa fa-angle-up"></i></div>
    @*<a id="top"></a>*@
    <!-- Header Section -->
    <header id="header" class="header">
        @Html.Action("GetMenus", "Blog", new { @Area = "" })
    </header>
    <!-- Header Section /- -->
    <!-- Latest Updates -->
    @Html.Action(actionName: "LatestUpdate", controllerName: "Blog")
    <!-- Latest Updates /- -->
    @RenderBody()
    <!-- Footer Section -->
    <div class="clearfix"></div>
    @Html.Action("GetFooter", "Blog", new { @Area = "" })
    <!-- Footer Section /- -->
    <!-- jQuery Include -->
    @Scripts.Render("~/bundles/blogcjs")
    @Scripts.Render("~/persiaNumberJs")
    @RenderSection("scripts", required: false)
    <script src="@Url.Content("~/Plugins/cool-share/plugin.js")"></script>
    <script src="@Url.Content("~/Plugins/cool-share/shareconfig.js")"></script>
    <script>
        $(document).ready(function () {
            var cond = $(".navbar-toggle").css('display') == 'block';

            if (cond) {
                (function ($) {
                    $(document).ready(function () {
                        $('ul.dropdown-menu [data-toggle=dropdown]').on('click', function (event) {
                            event.preventDefault();
                            event.stopPropagation();
                            $(this).parent().siblings().removeClass('open');
                            $(this).parent().toggleClass('open');
                        });
                    });
                })(jQuery);
            }
        })
    </script>
    <script>
        $(window).scroll(function () {
            var height = $(window).scrollTop();
            var middle = ($(document).height() / 2);
            if (height >= middle) {
                $("#scroll-top").show();
            } else {
                $("#scroll-top").hide();
            }
        });
        $("#scroll-top").on("click", function () {
            $('html, body').animate({
                scrollTop: 0
            });
        });
    </script>
